{
  "name": "yiisoft/yii2-app-basic",
  "description": "Yii 2 Basic Project Template",
  "keywords": ["yii2", "framework", "basic", "project template"],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "~2.0.5",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "nesbot/carbon": "^1.22",
    "insolita/yii2-migration-generator": "~2.3",
    "mbrowniebytes/yii2-clean-vendors": "dev-master",
    "bryglen/yii2-sendgrid": "*",
    "undefinedor/yii2-cached-active-record": "^0.0.3",
    "yiisoft/yii2-faker": "^2.0",
    "tebazil/yii2-console-runner": "^0.0.0",
    "omnilight/yii2-scheduling": "*",
    "skeeks/yii2-assets-auto-compress": "*",
    "mootensai/yii2-enhanced-gii": "dev-master",
    "kartik-v/yii2-tree-manager": "dev-master",
    "johnitvn/yii2-ajaxcrud": "*",
    "warrence/yii2-kartikgii": "dev-master",
    "setasign/fpdf": "1.8.1",
    "kartik-v/yii2-widget-growl": "*",
    "wbraganca/yii2-dynamicform": "dev-master",
    "kartik-v/yii2-date-range": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",

    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3",
    "jamband/yii2-schemadump": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset":{
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ],
    "post-update-cmd": [
      "php yii clean-vendors"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}